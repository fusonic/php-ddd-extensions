{
    "name": "fusonic/http-kernel-extensions",
    "version": "1.0.2",
    "description": "Symfony HttpKernel Component Extensions.",
    "type": "library",
    "authors": [
        {
            "name": "Fusonic GmbH",
            "email": "office@fusonic.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fusonic\\HttpKernelExtensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fusonic\\HttpKernelExtensions\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^0.12.33",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "require": {
        "php": ">=7.4",
        "symfony/http-kernel": "^4.4",
        "symfony/serializer": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/property-info": "^4.4",
        "symfony/property-access": "^4.4",
        "doctrine/cache": "^1.10",
        "doctrine/annotations": "^1.10"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "phpcs-check": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist -v --dry-run --diff --using-cache=yes",
        "phpcs-fix": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist -v --using-cache=yes"
    }
}
